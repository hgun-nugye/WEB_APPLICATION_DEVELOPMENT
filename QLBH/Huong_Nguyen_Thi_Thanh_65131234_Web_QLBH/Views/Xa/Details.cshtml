@model Huong_Nguyen_Thi_Thanh_65131234_Web_QLBH.Models.Xa
@{
    ViewBag.Title = "Chi tiết Xã";
}

<h2 class="text-info mb-3">
    <i class="fa-solid fa-eye"></i> Chi tiết Xã
</h2>

<div class="card shadow-sm border-info">
    <div class="card-body">
        <p><strong><i class="fa-solid fa-hashtag"></i> Mã Xã:</strong> @Model.MaXa</p>
        <p><strong><i class="fa-solid fa-id-badge"></i> Tên Xã:</strong> @Model.TenXa</p>
        <p><strong><i class="fa-solid fa-id-badge"></i> Mã Tỉnh:</strong> @Model.MaTinh</p>
    </div>
</div>

<a asp-action="Xa_Admin" class="btn btn-secondary mt-3">
    <i class="fa-solid fa-arrow-left"></i> Quay lại
</a>
